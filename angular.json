{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "parser": {
      "root": "libs/parser",
      "sourceRoot": "libs/parser/src",
      "projectType": "library",
      "prefix": "flogo-web",
      "architect": {
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/parser/jest.config.js",
            "tsConfig": "libs/parser/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/parser/tsconfig.lib.json",
              "libs/parser/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "server": {
      "root": "apps/server",
      "sourceRoot": "apps/server/src",
      "projectType": "application",
      "prefix": "server",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/builders:node-build",
          "options": {
            "outputPath": "dist/apps/server",
            "main": "apps/server/src/server.ts",
            "tsConfig": "apps/server/tsconfig.app.json",
            "assets": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/server/src/environments/environment.ts",
                  "with": "apps/server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "./tools/builders:dev-server",
          "options": {
            "buildTarget": "server:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/server/tsconfig.app.json",
              "apps/server/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/server/jest.config.js",
            "tsConfig": "apps/server/tsconfig.spec.json"
          }
        }
      }
    },
    "client": {
      "root": "apps/client/",
      "sourceRoot": "apps/client/src",
      "projectType": "application",
      "prefix": "flogo-web",
      "schematics": {
        "@schematics/angular:component": {
          "prefix": "flogo",
          "styleext": "less"
        },
        "@schematics/angular:directive": {
          "prefix": "fg"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/client",
            "index": "apps/client/src/index.html",
            "main": "apps/client/src/main.ts",
            "polyfills": "apps/client/src/polyfills.ts",
            "tsConfig": "apps/client/tsconfig.app.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "apps/client/src"
              ]
            },
            "assets": [
              "apps/client/src/favicon.ico",
              "apps/client/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor/min",
                "output": "/assets/monaco-editor"
              },
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor/min-maps",
                "output": "/assets/min-maps"
              },
              {
                "glob": "**/*",
                "input": "apps/client/src/i18n",
                "output": "/i18n"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
              "node_modules/@angular/cdk/overlay-prebuilt.css",
              "apps/client/src/assets/ui-pattern/css/tibco-cloud-ui-pattern.css",
              "apps/client/src/styles.less"
            ],
            "scripts": [
              "node_modules/lodash/lodash.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/client/src/environments/environment.ts",
                  "with": "apps/client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "client:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/client/src/test.ts",
            "polyfills": "apps/client/src/polyfills.ts",
            "tsConfig": "apps/client/tsconfig.spec.json",
            "karmaConfig": "apps/client/karma.conf.js",
            "stylePreprocessorOptions": {
              "includePaths": [
                "apps/client/src"
              ]
            },
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
              "node_modules/@angular/cdk/overlay-prebuilt.css",
              "apps/client/src/assets/ui-pattern/css/tibco-cloud-ui-pattern.css",
              "apps/client/src/styles.less"
            ],
            "scripts": [
              "node_modules/lodash/lodash.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ],
            "assets": [
              "apps/client/src/favicon.ico",
              "apps/client/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor/min",
                "output": "/assets/monaco-editor"
              },
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor/min-maps",
                "output": "/assets/min-maps"
              },
              {
                "glob": "**/*",
                "input": "apps/client/src/i18n",
                "output": "/i18n"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/client/tsconfig.app.json",
              "apps/client/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "client-e2e": {
      "root": "apps/client-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/client-e2e/protractor.conf.js",
            "devServerTarget": "client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/client-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "server-core": {
      "root": "libs/server/core",
      "sourceRoot": "libs/server/core/src",
      "projectType": "library",
      "prefix": "flogo-web",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/server/core/tsconfig.lib.json",
              "libs/server/core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/server/core/jest.config.js",
            "tsConfig": "libs/server/core/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "plugins-flow-server": {
      "root": "libs/plugins/flow-server",
      "sourceRoot": "libs/plugins/flow-server/src",
      "projectType": "library",
      "prefix": "flogo-web",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/plugins/flow-server/tsconfig.lib.json",
              "libs/plugins/flow-server/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/plugins/flow-server/jest.config.js",
            "tsConfig": "libs/plugins/flow-server/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "client-core": {
      "root": "libs/client/core",
      "sourceRoot": "libs/client/core/src",
      "projectType": "library",
      "prefix": "flogo-web",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/client/core/tsconfig.lib.json",
            "project": "libs/client/core/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/client/core/src/test.ts",
            "tsConfig": "libs/client/core/tsconfig.spec.json",
            "karmaConfig": "libs/client/core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/client/core/tsconfig.lib.json",
              "libs/client/core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "client-shared": {
      "root": "libs/client/shared",
      "sourceRoot": "libs/client/shared/src",
      "projectType": "library",
      "prefix": "flogo-web",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/client/shared/tsconfig.lib.json",
            "project": "libs/client/shared/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/client/shared/src/test.ts",
            "tsConfig": "libs/client/shared/tsconfig.spec.json",
            "karmaConfig": "libs/client/shared/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/client/shared/tsconfig.lib.json",
              "libs/client/shared/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics",
    "packageManager": "yarn"
  },
  "defaultProject": "client"
}
